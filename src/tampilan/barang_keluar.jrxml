	<queryString language="SQL">
		<![CDATA[SELECT
     barang_keluar.`no_transaksi` AS barang_keluar_no_transaksi,
     barang_keluar.`tanggal` AS barang_keluar_tanggal,
     barang_keluar.`kode_barang` AS barang_keluar_kode_barang,
     barang_keluar.`jumlah` AS barang_keluar_jumlah,
     barang_keluar.`tujuan` AS barang_keluar_tujuan,
     barang.`id` AS barang_id,
     barang.`kode_barang` AS barang_kode_barang,
     barang.`nama_barang` AS barang_nama_barang,
     barang.`kode_kategori` AS barang_kode_kategori,
     barang.`kode_lokasi` AS barang_kode_lokasi,
     barang.`stok` AS barang_stok,
     barang.`harga` AS barang_harga
FROM
     `barang` barang INNER JOIN `barang_keluar` barang_keluar ON barang.`kode_barang` = barang_keluar.`kode_barang`]]>
	</queryString>
